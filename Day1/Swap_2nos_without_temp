# include <stdio.h>
void main()
{
 int a,b;
 printf("Enter number1:\n");
 scanf("%d", &a);
 printf("Enter number2:\n");
 scanf("%d", &b);
 printf("The value of number1 before swap is:%d \n",a);
 printf("The value of number2 before swap is:%d \n",b);
 a = a - b;
 b = a + b;
 a = b - a;
 printf("The value of number1 after swap is:%d \n",a);
 printf("The value of number2 after swap is:%d \n",b);
}

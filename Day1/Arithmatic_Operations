# include <stdio.h>
void main()
{
 double a,b,c;
 int d=5,e=2;
 printf("Enter number1:\n");
 scanf("%lf", &a);
 printf("Enter number2:\n");
 scanf("%lf", &b);
 c = a+b;
 printf("Sum of two numbers is: %lf", c);
 c = a-b;
 printf("\nDifference of two numbers is: %lf", c);
 c = a*b;
 printf("\nProduct of two numbers is: %lf", c);
 c = a/b;
 printf("\nQuotient of two numbers is: %lf", c);
 c = d%e;
 printf("\nReminder of two numbers is: %lf", c);
}

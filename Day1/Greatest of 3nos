#include <stdio.h>

int main() {
    int a,b,c;
    printf("****Finding greatest of three numbers****");
    printf("\nEnter three numbers seperated by space:");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    printf("\nThe greatst number is %d",a);
    else if(b>a && b>c)
    printf("\nThe greatst number is %d",b);
    else
    printf("\nThe greatst number is %d",c);
    return 0;
}
